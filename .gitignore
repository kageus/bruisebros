# Minecraft Server .gitignore

# World directories (backup these separately!)
world/
realworld/
survivalworld/
world_nether/
world_the_end/

# Logs
logs/

# Server JARs (large, easily re-downloadable)
*.jar

# Libraries (can be regenerated)
libraries/
versions/

# Temporary files
usercache.json

# Crash reports
crash-reports/

# Plugin binaries
plugins/

# Mod binaries and runtime data
mods/*.jar
mods/**/*.jar
mods/**/data/
mods/**/cache/

# BUT track mod configs
!config/

# Claude Code local settings
.claude/settings.local.json

# Console history
.console_history
